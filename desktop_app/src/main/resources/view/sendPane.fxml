<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>

<Pane fx:id="sendPane" prefHeight="350.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tambapps.p2p.fandem.desktop.controller.SendPaneController">
   <stylesheets>
      <URL value="@../style/style.css" />
   </stylesheets>
   <children>
      <Label layoutX="165.0" layoutY="30.0" styleClass="title" text="Send File(s)" />
      <Button fx:id="pickFileButton" layoutX="203.0" layoutY="159.0" mnemonicParsing="false" onAction="#pickFile" text="Pick file" />
      <Label fx:id="dragDropLabel" layoutX="126.0" layoutY="210.0" prefHeight="29.0" prefWidth="247.0" text="or drag and drop file(s) here" textAlignment="CENTER" />
   </children>
</Pane>
